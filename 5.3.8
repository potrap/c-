#include <iostream>
#include <string>
#include<cmath>

/*
   Відстань від точки до точки
*/

using namespace std;

class Point2D {

public:
	Point2D(double x, double y) {
		this->x = x;
		this->y = y;
	}

	string toString();

	double toDouble();

	double distanceTo(Point2D that) {
		double distance = sqrt(pow((this->x - that.x), 2) + pow((this->y - that.y),2));
		return distance;
	};

private:

	double x;
	double y;
};

Point2D createPoint(string line) {
	string res = "";
	bool check = true;
	double x = 0, y = 0;
	for (int i = 0; i <= line.length(); i++) {
		if (check) {
			if (line[i] != ',') {
				res = res + line[i];
			} else {
				x = stoi(res);
				res = "";
				check = false;
			}
		} else {
			if (i != line.length()) {
				res = res + line[i];
			}
			else {
				y = stoi(res);
			}
		}
		
	}
	Point2D result(x, y);
	return result;
}

int main(void) {

	string line1 = "";
	cin >> line1;
	Point2D first = createPoint(line1);

	string line2 = "";
	cin >> line2;
	Point2D second = createPoint(line2);

	cout << first.distanceTo(second);
	
}
